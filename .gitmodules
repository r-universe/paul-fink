[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "impimp"]
	path = impimp
	url = https://github.com/cran/impimp
	shallow = true
	registered = true
[submodule "imptree"]
	path = imptree
	url = https://github.com/paul-fink/imptree
	shallow = true
	registered = true
